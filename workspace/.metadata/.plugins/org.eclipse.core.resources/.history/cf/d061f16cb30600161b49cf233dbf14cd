
public class HelloWorld {

	public static void main (String[] args){
		int x =0; 
		
		while (x <= 20){
			
			System.out.print("Sweet \n");
			
			x++;
			
		}
		
	
	System.out.println("Hello world");
		
	int s = sum(5,6);
		
	System.out.println(s);
	
	}
	
	static int sum(int x, int y){
		
		
		
		return x+y;
	}
	
}
